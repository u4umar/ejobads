{#
/**
 * @file
 * AdSense responsive managed ad HTML code.
 */
#}
{% if not defer %}
<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-{{ client }}"
     crossorigin="anonymous"></script>
{% else %}
  {{ attach_library('adsense/adsense.adsbygoogle') }}
{% endif %}
<!-- {{ format }} -->
<ins class="adsbygoogle"
     style="display:block"
     data-ad-client="ca-{{ client }}"
     data-ad-slot="{{ slot }}"
     data-ad-format="{{ shape }}"
     data-full-width-responsive="true"></ins>
<script>
     (adsbygoogle = window.adsbygoogle || []).push({});
</script>